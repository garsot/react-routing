{
  "name": "@kemsu/react-routing",  
  "version": "1.0.0-alpha.0",
  "description": "Routing for React using hooks",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint": "npx eslint ./src",
    "prepublishOnly": "npm run build",
    "build": "rollup -c --environment NODE_ENV:production",
    "start": "rollup -c -w --environment RUN_EXAMPLE,NODE_ENV:development",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garsot/react-routing.git"
  },
  "keywords": [
    "react",
    "routing",
    "hooks"
  ],
  "author": {
    "name": "Igor Sotnikov",
    "email": "sotnikov@kemsu.ru"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garsot/react-routing/issues"
  },
  "homepage": "https://github.com/garsot/react-routing#readme",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@types/jest": "^24.0.11",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.3",
    "rollup-plugin-copy": "^0.3.0",
    "rollup-plugin-node-resolve-with-alias": "^3.0.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-serve": "^1.0.1"
  },
  "babelOptions": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "files": [
    "dist"
  ]
}
